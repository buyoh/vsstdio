{
  "name": "vsstdio",
  "displayName": "shell runner with stdio",
  "description": "",
  "version": "0.0.1",
  "publisher": "buyoh",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsstdio.helloWorld",
    "onView:vsstdio.commandPanelView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [  ],
    "viewsContainers": {
      "panel": [
        {
          "id": "vsstdio",
          "title": "vsstdio",
          "icon": "resources/img/787_me_f.svg"
        }
      ]
    },
    "views": {
      "vsstdio": [
        {
          "type": "webview",
          "id": "vsstdio.commandPanelView",
          "name": "shell runner"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "eslint": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vscode-test": "^1.6.1",
    "ts-loader": "^9.2.6",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
